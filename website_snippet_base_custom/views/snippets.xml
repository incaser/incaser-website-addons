<openerp>
<data>

<template id="snippet_base_custom" inherit_id="website.snippets">
	<xpath expr="//ul" position="inside">
		<li>
			<a href="#snippet_base_custom" class="scroll-link">
				<span class="fa-stack fa-lg">
					<i class="fa fa-square fa-stack-2x fa-inverse"></i>
					<i class="fa fa-user fa-stack-1x"></i>
				</span>
			</a>
		</li>
	</xpath>
	<xpath expr="//div[@id='o_scroll']" position="inside">
		<div id="snippet_base_custom" class="o_panel">
			<div class="o_panel_header">
				<span class="fa-stack">
					<i class="fa fa-user fa-stack-1x"></i>
				</span> Custom Snippets
			</div>
			<div id="custom_snippet" class="o_panel_body">
				<!--To adds snippets by other modules-->
			</div>
		</div>
	</xpath>
</template>

</data>
</openerp>
